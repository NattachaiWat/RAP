layer {
  name: "data"
  type: "MultiImageData"
  top: "data"
  top: "label"
  include {
    phase: TEST
  }
  transform_param {
    mirror: false 
    scale: 0.00390625
    # crop_size: 224
    mean_value: 104
    mean_value: 117
    mean_value: 123
  }
  multiimage_data_param {
    # root_folder: "/mnt/data1/lidw/dataset/"
    root_folder: "./../../data/RAP_dataset/"
    source: "./../static/classification/rap2_ide_val.txt"
    shuffle: false 
    batch_size: 128 
    new_height: 160
    new_width: 64
  }
}
